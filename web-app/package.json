      "@babel/plugin-proposal-class-properties": "^7.18.6",
      "@babel/preset-env": "^7.18.10",
      "@babel/preset-react": "^7.18.6",
      "@material-ui/core": "^4.12.4",
      "@material-ui/icons": "^4.11.3",
      "axios": "^0.27.2",
      "classnames": "^2.3.1",
      "connected-react-router": "^6.9.3",
      "fuse.js": "^6.6.2",
      "history": "^5.0.0",
      "howler": "^2.2.3",
      "mousetrap": "^1.6.5",
      "node-sass": "^7.0.1",
      "prop-types": "^15.8.1",
      "react": "^18.2.0",
      "react-dnd": "^13.1.0",
      "react-dnd-html5-backend": "^12.1.0",
      "react-dom": "^18.2.0",
      "react-icons": "^4.4.0",
      "react-redux": "^8.0.2",
      "react-router-dom": "^5.3.3",
      "react-scripts": "4.0.3",
      "react-select-search": "^3.0.10",
